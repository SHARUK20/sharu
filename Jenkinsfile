        {
            steps
            {
                sh 'touchabc.txt'
            }
        }
        stage('testing the code')
        {
            steps
            {
                sh 'touchabc.txt'
            }
        }
        stage('deploye the code')
        {
            steps
            {
                sh 'touchabc.txt'
            }
        }
    }
